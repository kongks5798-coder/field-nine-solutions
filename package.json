    "prisma:push": "prisma db push",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "ai:export": "ts-node --compiler-options {\"module\":\"CommonJS\"} scripts/ai-training-data-export.ts",
    "ai:train": "python scripts/ai-forecast.py --product-id demo-product --timeframe weekly",
    "ai:test": "npm run test && npm run ai:export",
    "build:vercel": "next build || exit 0",
    "predeploy": "npm run build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "finalize:100": "ts-node --compiler-options {\"module\":\"CommonJS\"} scripts/finalize-100.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
