{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["icn1"],
  "crons": [
    {
      "path": "/api/naver/crawl?crawlAll=true",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/energy/cron",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/kaus/staking/cron",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/kaus/dividend/cron",
      "schedule": "0 3 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "nexus.fieldnine.io" }],
      "destination": "/ko/nexus"
    },
    {
      "source": "/pro",
      "has": [{ "type": "host", "value": "nexus.fieldnine.io" }],
      "destination": "/ko/nexus/pro"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "nexus.fieldnine.io" }],
      "destination": "/ko/nexus/:path*"
    },
    {
      "source": "/",
      "has": [{ "type": "host", "value": "m.fieldnine.io" }],
      "destination": "/ko"
    },
    {
      "source": "/api/:path*",
      "has": [{ "type": "host", "value": "m.fieldnine.io" }],
      "destination": "/api/:path*"
    },
    {
      "source": "/nexus/:path*",
      "has": [{ "type": "host", "value": "m.fieldnine.io" }],
      "destination": "/ko/nexus/:path*"
    },
    {
      "source": "/dashboard/:path*",
      "has": [{ "type": "host", "value": "m.fieldnine.io" }],
      "destination": "/ko/dashboard/:path*"
    },
    {
      "source": "/vrd/:path*",
      "has": [{ "type": "host", "value": "m.fieldnine.io" }],
      "destination": "/ko/vrd/:path*"
    },
    {
      "source": "/",
      "has": [{ "type": "host", "value": "portal.fieldnine.io" }],
      "destination": "/ko/portal"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "portal.fieldnine.io" }],
      "destination": "/ko/:path*"
    },
    {
      "source": "/",
      "has": [{ "type": "host", "value": "nomad.fieldnine.io" }],
      "destination": "/ko/dashboard"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "nomad.fieldnine.io" }],
      "destination": "/ko/dashboard/:path*"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "api.fieldnine.io" }],
      "destination": "/api/:path*"
    }
  ]
}
